{
    "name": "codexpedite/laravel-error-reporter",
    "description": "Laravel package to automatically report errors to webhook endpoints",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CodeXpedite",
            "email": "info@codexpedite.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "CodeXpedite\\ErrorReporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeXpedite\\ErrorReporter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeXpedite\\ErrorReporter\\ErrorReporterServiceProvider"
            ],
            "aliases": {
                "ErrorReporter": "CodeXpedite\\ErrorReporter\\Facades\\ErrorReporter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}